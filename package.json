{
  "name": "phobrary",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "ci-test": "npm run test:e2e:build && npm run test",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --passWithNoTests /src/.*/__tests__/*",
    "test:e2e": "detox test -c ios.sim.debug",
    "test:e2e:build": "detox build",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/cameraroll": "^4.0.2",
    "babel-plugin-rewrite-require": "^1.14.5",
    "buffer": "^5.4.3",
    "crypto-js": "^3.1.9-1",
    "deep-equal": "^1.1.1",
    "node-libs-browser": "^2.2.1",
    "otplib": "^11.0.1",
    "react": "16.13.1",
    "react-native": "^0.63.4",
    "react-native-device-info": "^3.1.4",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-zeroconf": "^0.13.1",
    "react-navigation": "^3.12.1",
    "react-redux": "^7.1.3",
    "react-timeout": "^1.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "urijs": "^1.19.6",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-fetch": "^3.0.6",
    "detox": "^18.6.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.8.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "mockserver-client": "^5.11.1",
    "mockserver-node": "^5.11.1",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "jest-junit": {
    "outputDirectory": "./artifacts/jest"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/phobrary.app",
        "build": "set -o pipefail && xcodebuild -workspace ios/phobrary.xcworkspace -UseNewBuildSystem=NO -scheme phobrary -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -quiet",
        "type": "ios.simulator",
        "name": "iPhone 8"
      }
    }
  }
}
